    </configuration>
</plugin>
		    <plugin>
		        <groupId>com.microsoft.azure</groupId>
		        <artifactId>azure-spring-apps-maven-plugin</artifactId>
		        <version>1.19.0</version>
		        <configuration>
		            <subscriptionId>e7c08068-7144-4272-a5a1-1a4f02823410</subscriptionId>
		            <resourceGroup>catchtalentressource</resourceGroup>
		            <clusterName>catchtalentapi</clusterName>
		            <sku>Basic</sku>
		            <appName>simple-todo-api</appName>
		            <isPublic>true</isPublic>

		            <deployment>
		                <cpu>1.0</cpu>
		                <memoryInGB>2.0</memoryInGB>
		                <instanceCount>1</instanceCount>
		                <runtimeVersion>Java_17</runtimeVersion>

		                <resources>
		                    <resource>
		                        <directory>${project.basedir}/target</directory>
		                        <includes>
		                            <include>*.jar</include>
		                        </includes>
		                    </resource>
		                </resources>
		            </deployment>
		        </configuration>
		    </plugin>
